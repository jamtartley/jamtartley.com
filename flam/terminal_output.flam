<div class="max-h-full mb-4">
	{! include "./terminal_prompt.flam", command: item.command !}
	{! if item -> has("link") !}
	<a class="block text-pink underline" href="{= item.link =}" target="_blank">{= item.output =}</a>
	{! else !}
	<p class="text-white text-base">{= item.output =}</p>
	{! fi !}
	{! if item -> has("image") !}
	<img class="w-full h-auto my-3" src="{= item.image =}" />
	{! fi !}
</div>
